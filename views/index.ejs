<body>


	<div class="container-contact100">
		<div class="wrap-contact100">
			<form class="contact100-form validate-form">
				<span class="contact100-form-title">
					Cấp phát và quản lý thẻ công dân
				</span>

				<div class="wrap-input100 validate-input bg1" data-validate="Please Type Your Name">
					<span class="label-input100">Họ và tên*</span>
					<input class="input100 TenHK" type="text" name="name" placeholder="Tiếng việt có dấu">
				</div>

				<div class="wrap-input100 validate-input bg2" data-validate="Please Type Your Name">
					<span class="label-input100">Họ và tên khác (nếu có)</span>
					<input class="input100 anotherName" type="text" name="name"
						placeholder="Có thể bỏ trống hoặc nhập tên khác theo sổ hộ khẩu">
				</div>

				<div class="wrap-input100 validate-input bg1 rs1-wrap-input100"
					data-validate="Enter Your Email (e@a.x)">
					<span class="label-input100">Giới Tính</span>
					<input class="input100 GT" type="text" name="text" placeholder="Nam/Nữ">
				</div>

				<div class="wrap-input100 bg1 rs1-wrap-input100">
					<span class="label-input100">Ngày, tháng, năm sinh</span>
					<input class="input100 NS" type="text" name="Date" placeholder="Nhập theo định dạng ngày/tháng/năm">
				</div>

				<div class="wrap-input100 validate-input bg4" data-validate="Please Type Your Name">
					<span class="label-input100">Quê quán</span>
					<input class="input100 QQ" type="text" name="name" placeholder="Tiếng việt có dấu">
				</div>

				<div class="wrap-input100 validate-input bg1" data-validate="Please Type Your Name">
					<span class="label-input100">Nơi thường trú</span>
					<input class="input100 T_TRU" type="text" name="name" placeholder="Tiếng việt có dấu">
				</div>

				<div class="wrap-input100 validate-input bg3" data-validate="Please Date">
					<span class="label-input100">Ngày Lên Nộp</span>
					<input class="input100" type="text" name="Date" placeholder="Nhập theo định dạng ngày/tháng/năm">
				</div>

				<div class="wrap-input100 validate-input bg1 rs1-wrap-input100">
					<span class="label-input100">Ngày </span>
					<input class="input100" type="text" name="text" placeholder="Nhập theo định dạng ngày/tháng/năm">
				</div>

				<div class="wrap-input100 bg1 rs1-wrap-input100">
					<span class="label-input100">Nơi cấp</span>
					<input class="input100 NoiCap" type="text" name="Date" placeholder="Nơi cấp">
				</div>

				<div class="wrap-input100 input100-select bg2">
					<span class="label-input100">Nghề Nghiệp*</span>
					<div>
						<select class="js-select2" name="service">
							<option>Học sinh</option>
							<option>Sinh viên</option>
							<option>Người đi làm</option>
						</select>
						<div class="dropDownSelect2"></div>
					</div>
				</div>
				<div class="wrap-input100 validate-input bg1 rs1-alert-validate"
					data-validate="Please Type Your Message">
					<span class="label-input100">Góp ý</span>
					<textarea class="input100" name="message"
						placeholder="Hãy đóng góp ý kiến của bạn để chúng tôi hoàn thiện dịch vụ hơn..."></textarea>
				</div>

				<div class="container-contact100-form-btn">
					<div class="contact100-form-btn" id="button">
						<span>
							Submit
							<i class="fa fa-long-arrow-right m-l-7" aria-hidden="true"></i>
						</span>
					</div>
				</div>
			</form>
		</div>
	</div>



	<!--===============================================================================================-->
